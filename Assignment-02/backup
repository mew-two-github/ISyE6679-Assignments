		// a[i] = a[i]/1000 ;
		
		// b[i] = (float)rand()/(float)(RAND_MAX/m);
		// b[i] = (int)( b[i]*1000); 
		// b[i] = b[i]/1000 ;
				for( int i = 0; i < r; i++ )
		{
		    for(int j = 0; j < c; j++)
		    {
		         printf("%f     ", a[i*c + j]);
		    }
		    printf("\n");
		}
		printf("\n");
		for( int i = 0; i < c; i++ )
		{
		    for(int j = 0; j < r; j++)
		    {
		         printf("%f     ", b[i*r + j]);
		    }
		    printf("\n");
		}
		printf("\n");